{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "DefaultUser": "System",
    "UrlValidationRegex": "^(?:(https?|ftps?):\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$",
    //"UrlValidationRegex": "^((https?|ftp|ftps):\/\/)?(www.)?[a-z0-9]+\.[a-z]+(\/[a-zA-Z0-9#]+\/?)*$"
    //"UrlValidationRegex": "^((https?|ftp|ftps):\/\/)?(www.)?[a-zA-Z0-9\-\.]+\.[a-zA-Z](:[a-zA-Z0-9]*)?/?([a-zA-Z0-9\-\._\?\,\'/\\\+&amp;%\$#\=~])*[^\.\,\)\(\s]$"
    //"UrlValidationRegex": "^(https?\://|ftps?\://)?|[a-zA-Z0-9\-\.]+\.[a-zA-Z](:[a-zA-Z0-9]*)?/?([a-zA-Z0-9\-\._\?\,\'/\\\+&amp;%\$#\=~])*[^\.\,\)\(\s]$"
    "ShortCodeMinLength": 3,
    "ShortCodeMaxLength": 8,
    "ShortCodeCharacterSet": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890",
    "NumberOfTryIfDuplicateShortCode": 50,
    "ExpirationOfShortUrlInMonths": 6
  },
  "ConnectionStrings": {
    "ShortUlrDbConnection": "Data Source=MFH;database=ShortUrlDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
